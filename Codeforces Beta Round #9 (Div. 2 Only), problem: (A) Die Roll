#include <bits/stdc++.h>

using namespace std;

int main() {
 
 	int arr[6]={1,2,3,4,5,6};
 	int y,w,max,cnt=0;
 	
 	
 	cin>>y>>w;
 	
 	if(y>w){
 		max=y;
	 }else{
	 	max=w;
	 }
 	
 	for(int i=0;i<6;i++){
 		if(arr[i]>= max){
 			cnt++;
		 }
	 }
 	
 	switch (cnt){
 		case(0):cout<<"0/1\n";
 		break;
 		case(1):cout<<"1/6\n";
 		break;
 		case(2):cout<<"1/3\n";
 		break;
 		case(3):cout<<"1/2\n";
 		break;
 		case(4):cout<<"2/3\n";
 		break;
 		case(5):cout<<"5/6\n";
 		break;
 		case(6):cout<<"1/1\n";
 		break;
	 }
 	
 	return 0;
 }
